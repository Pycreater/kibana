{
  "$schema": "https://raw.githubusercontent.com/nrwl/nx/refs/heads/master/packages/nx/schemas/project-schema.json",
  "name": "@kbn/alerting-example-plugin",
  "namedInputs": {
    "src": [
      "{projectRoot}/index.ts",
      "{projectRoot}/public/**/*.ts",
      "{projectRoot}/public/**/*.tsx",
      "{projectRoot}/server/**/*.ts",
      "{projectRoot}/common/**/*.ts",
      "{workspaceRoot}/typings/**/*",
      "!{projectRoot}/target/**/*"
    ]
  },
  "targets": {
    "typecheck": {
      "inputs": [
        "src",
        "^src",
        "global:tsConfigs",
        "^global:tsConfigs"
      ],
      "outputs": [
        "{projectRoot}/target"
      ]
    }
  },
  "projectType": "application",
  "sourceRoot": "x-pack/examples/alerting_example",
  "implicitDependencies": [
    "@kbn/core",
    "@kbn/kibana-react-plugin",
    "@kbn/charts-plugin",
    "@kbn/data-plugin",
    "@kbn/alerting-plugin",
    "@kbn/triggers-actions-ui-plugin",
    "@kbn/features-plugin",
    "@kbn/developer-examples-plugin",
    "@kbn/i18n",
    "@kbn/core-application-common",
    "@kbn/shared-ux-router",
    "@kbn/config-schema",
    "@kbn/alerts-as-data-utils"
  ]
}