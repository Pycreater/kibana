{
  "$schema": "https://raw.githubusercontent.com/nrwl/nx/refs/heads/master/packages/nx/schemas/project-schema.json",
  "name": "@kbn/alerting-fixture-plugin",
  "namedInputs": {
    "src": [
      "{projectRoot}/**/*.ts",
      "{projectRoot}/**/*.tsx",
      "!{projectRoot}/target/**/*"
    ]
  },
  "targets": {
    "typecheck": {
      "inputs": [
        "src",
        "^src",
        "global:tsConfigs",
        "^global:tsConfigs"
      ],
      "outputs": [
        "{projectRoot}/target"
      ]
    }
  },
  "projectType": "application",
  "sourceRoot": "x-pack/test/functional_with_es_ssl/plugins/alerts",
  "implicitDependencies": [
    "@kbn/core",
    "@kbn/features-plugin",
    "@kbn/alerting-plugin",
    "@kbn/triggers-actions-ui-plugin",
    "@kbn/shared-ux-router",
    "@kbn/react-kibana-context-render"
  ]
}