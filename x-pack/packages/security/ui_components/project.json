{
  "$schema": "https://raw.githubusercontent.com/nrwl/nx/refs/heads/master/packages/nx/schemas/project-schema.json",
  "name": "@kbn/security-ui-components",
  "namedInputs": {
    "src": [
      "{projectRoot}/**/*.ts",
      "{projectRoot}/**/*.tsx",
      "!{projectRoot}/target/**/*"
    ]
  },
  "targets": {
    "typecheck": {
      "inputs": [
        "src",
        "^src",
        "global:tsConfigs",
        "^global:tsConfigs"
      ],
      "outputs": [
        "{projectRoot}/target"
      ]
    }
  },
  "projectType": "library",
  "sourceRoot": "x-pack/packages/security/ui_components",
  "implicitDependencies": [
    "@kbn/core",
    "@kbn/i18n",
    "@kbn/i18n-react",
    "@kbn/security-plugin-types-common",
    "@kbn/test-jest-helpers",
    "@kbn/security-role-management-model",
    "@kbn/features-plugin"
  ]
}