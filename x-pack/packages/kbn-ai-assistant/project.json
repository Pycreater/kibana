{
  "$schema": "https://raw.githubusercontent.com/nrwl/nx/refs/heads/master/packages/nx/schemas/project-schema.json",
  "name": "@kbn/ai-assistant",
  "namedInputs": {
    "src": [
      "{projectRoot}/**/*.ts",
      "{projectRoot}/**/*.tsx",
      "!{projectRoot}/target/**/*"
    ]
  },
  "targets": {
    "typecheck": {
      "inputs": [
        "src",
        "^src",
        "global:tsConfigs",
        "^global:tsConfigs"
      ],
      "outputs": [
        "{projectRoot}/target"
      ]
    }
  },
  "projectType": "library",
  "sourceRoot": "x-pack/packages/kbn-ai-assistant",
  "implicitDependencies": [
    "@kbn/core-http-browser",
    "@kbn/i18n",
    "@kbn/triggers-actions-ui-plugin",
    "@kbn/actions-plugin",
    "@kbn/i18n-react",
    "@kbn/ui-theme",
    "@kbn/core",
    "@kbn/observability-ai-assistant-plugin",
    "@kbn/security-plugin",
    "@kbn/user-profile-components",
    "@kbn/std",
    "@kbn/utility-types-jest",
    "@kbn/kibana-react-plugin",
    "@kbn/monaco",
    "@kbn/licensing-plugin",
    "@kbn/code-editor",
    "@kbn/ml-plugin",
    "@kbn/share-plugin",
    "@kbn/ai-assistant-common"
  ]
}