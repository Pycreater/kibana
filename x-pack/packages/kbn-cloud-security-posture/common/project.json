{
  "$schema": "https://raw.githubusercontent.com/nrwl/nx/refs/heads/master/packages/nx/schemas/project-schema.json",
  "name": "@kbn/cloud-security-posture-common",
  "namedInputs": {
    "src": [
      "{projectRoot}/**/*.ts",
      "{projectRoot}/**/*.tsx",
      "!{projectRoot}/target/**/*"
    ]
  },
  "targets": {
    "typecheck": {
      "inputs": [
        "src",
        "^src",
        "global:tsConfigs",
        "^global:tsConfigs"
      ],
      "outputs": [
        "{projectRoot}/target"
      ]
    }
  },
  "projectType": "library",
  "sourceRoot": "x-pack/packages/kbn-cloud-security-posture/common",
  "implicitDependencies": [
    "@kbn/config-schema",
    "@kbn/data-views-plugin",
    "@kbn/i18n",
    "@kbn/analytics",
    "@kbn/usage-collection-plugin",
    "@kbn/es-query"
  ]
}