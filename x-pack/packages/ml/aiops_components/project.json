{
  "$schema": "https://raw.githubusercontent.com/nrwl/nx/refs/heads/master/packages/nx/schemas/project-schema.json",
  "name": "@kbn/aiops-components",
  "namedInputs": {
    "src": [
      "{projectRoot}/**/*.ts",
      "{projectRoot}/**/*.tsx",
      "!{projectRoot}/target/**/*"
    ]
  },
  "targets": {
    "typecheck": {
      "inputs": [
        "src",
        "^src",
        "global:tsConfigs",
        "^global:tsConfigs"
      ],
      "outputs": [
        "{projectRoot}/target"
      ]
    }
  },
  "projectType": "library",
  "sourceRoot": "x-pack/packages/ml/aiops_components",
  "implicitDependencies": [
    "@kbn/i18n-react",
    "@kbn/i18n",
    "@kbn/core",
    "@kbn/charts-plugin",
    "@kbn/data-plugin",
    "@kbn/field-formats-plugin",
    "@kbn/visualization-utils",
    "@kbn/aiops-log-rate-analysis",
    "@kbn/ml-agg-utils"
  ]
}