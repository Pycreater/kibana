{
  "$schema": "https://raw.githubusercontent.com/nrwl/nx/refs/heads/master/packages/nx/schemas/project-schema.json",
  "name": "@kbn/elastic-assistant-common",
  "namedInputs": {
    "src": [
      "{projectRoot}/**/*.ts",
      "{projectRoot}/**/*.tsx",
      "!{projectRoot}/target/**/*"
    ]
  },
  "targets": {
    "typecheck": {
      "inputs": [
        "src",
        "^src",
        "global:tsConfigs",
        "^global:tsConfigs"
      ],
      "outputs": [
        "{projectRoot}/target"
      ]
    }
  },
  "projectType": "library",
  "sourceRoot": "x-pack/packages/kbn-elastic-assistant-common",
  "implicitDependencies": [
    "@kbn/zod-helpers",
    "@kbn/securitysolution-io-ts-utils",
    "@kbn/core",
    "@kbn/logging",
    "@kbn/zod"
  ]
}