{
  "$schema": "https://raw.githubusercontent.com/nrwl/nx/refs/heads/master/packages/nx/schemas/project-schema.json",
  "name": "@kbn/security-solution-features",
  "namedInputs": {
    "src": [
      "{projectRoot}/**/*.ts",
      "{projectRoot}/**/*.tsx",
      "!{projectRoot}/target/**/*"
    ]
  },
  "targets": {
    "typecheck": {
      "inputs": [
        "src",
        "^src",
        "global:tsConfigs",
        "^global:tsConfigs"
      ],
      "outputs": [
        "{projectRoot}/target"
      ]
    }
  },
  "projectType": "library",
  "sourceRoot": "x-pack/packages/security-solution/features",
  "implicitDependencies": [
    "@kbn/features-plugin",
    "@kbn/utility-types",
    "@kbn/i18n",
    "@kbn/core-application-common",
    "@kbn/cases-plugin",
    "@kbn/securitysolution-rules",
    "@kbn/securitysolution-list-constants"
  ]
}