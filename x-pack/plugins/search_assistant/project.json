{
  "$schema": "https://raw.githubusercontent.com/nrwl/nx/refs/heads/master/packages/nx/schemas/project-schema.json",
  "name": "@kbn/search-assistant",
  "namedInputs": {
    "src": [
      "{projectRoot}/index.ts",
      "{projectRoot}/common/**/*.ts",
      "{projectRoot}/public/**/*.ts",
      "{projectRoot}/public/**/*.tsx",
      "{projectRoot}/server/**/*.ts",
      "{workspaceRoot}/typings/**/*",
      "!{projectRoot}/target/**/*"
    ]
  },
  "targets": {
    "typecheck": {
      "inputs": [
        "src",
        "^src",
        "global:tsConfigs",
        "^global:tsConfigs"
      ],
      "outputs": [
        "{projectRoot}/target"
      ]
    }
  },
  "projectType": "application",
  "sourceRoot": "x-pack/plugins/search_assistant",
  "implicitDependencies": [
    "@kbn/core",
    "@kbn/kibana-react-plugin",
    "@kbn/shared-ux-page-kibana-template",
    "@kbn/usage-collection-plugin",
    "@kbn/observability-ai-assistant-plugin",
    "@kbn/config-schema",
    "@kbn/ai-assistant",
    "@kbn/i18n",
    "@kbn/serverless",
    "@kbn/react-kibana-context-theme",
    "@kbn/shared-ux-link-redirect-app",
    "@kbn/shared-ux-utility",
    "@kbn/core-lifecycle-browser",
    "@kbn/licensing-plugin",
    "@kbn/ml-plugin",
    "@kbn/share-plugin",
    "@kbn/triggers-actions-ui-plugin"
  ]
}