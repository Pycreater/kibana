{
  "$schema": "https://raw.githubusercontent.com/nrwl/nx/refs/heads/master/packages/nx/schemas/project-schema.json",
  "name": "@kbn/kubernetes-security-plugin",
  "namedInputs": {
    "src": [
      "{projectRoot}/common/**/*",
      "{projectRoot}/public/**/*",
      "{projectRoot}/server/**/*",
      "{projectRoot}/server/**/*.json",
      "{projectRoot}/scripts/**/*",
      "{projectRoot}/package.json",
      "{projectRoot}/storybook/**/*",
      "{workspaceRoot}/typings/**/*",
      "!{projectRoot}/target/**/*"
    ]
  },
  "targets": {
    "typecheck": {
      "inputs": [
        "src",
        "^src",
        "global:tsConfigs",
        "^global:tsConfigs"
      ],
      "outputs": [
        "{projectRoot}/target"
      ]
    }
  },
  "projectType": "application",
  "sourceRoot": "x-pack/plugins/kubernetes_security",
  "implicitDependencies": [
    "@kbn/core",
    "@kbn/data-plugin",
    "@kbn/kibana-react-plugin",
    "@kbn/rule-registry-plugin",
    "@kbn/session-view-plugin",
    "@kbn/i18n",
    "@kbn/timelines-plugin",
    "@kbn/es-query",
    "@kbn/ui-theme",
    "@kbn/i18n-react",
    "@kbn/config-schema",
    "@kbn/shared-ux-router",
    "@kbn/securitysolution-es-utils"
  ]
}