{
  "$schema": "https://raw.githubusercontent.com/nrwl/nx/refs/heads/master/packages/nx/schemas/project-schema.json",
  "name": "@kbn/task-manager-plugin",
  "namedInputs": {
    "src": [
      "{projectRoot}/server/**/*",
      "{projectRoot}/server/**/*.json",
      "{projectRoot}/common/**/*",
      "!{projectRoot}/target/**/*"
    ]
  },
  "targets": {
    "typecheck": {
      "inputs": [
        "src",
        "^src",
        "global:tsConfigs",
        "^global:tsConfigs"
      ],
      "outputs": [
        "{projectRoot}/target"
      ]
    }
  },
  "projectType": "application",
  "sourceRoot": "x-pack/plugins/task_manager",
  "implicitDependencies": [
    "@kbn/core",
    "@kbn/usage-collection-plugin",
    "@kbn/config-schema",
    "@kbn/config",
    "@kbn/utility-types",
    "@kbn/safer-lodash-set",
    "@kbn/es-types",
    "@kbn/apm-utils",
    "@kbn/core-saved-objects-common",
    "@kbn/core-saved-objects-utils-server",
    "@kbn/core-test-helpers-kbn-server",
    "@kbn/core-saved-objects-server",
    "@kbn/alerting-state-types",
    "@kbn/core-saved-objects-api-server",
    "@kbn/logging",
    "@kbn/core-lifecycle-server",
    "@kbn/cloud-plugin",
    "@kbn/core-saved-objects-base-server-internal",
    "@kbn/core-elasticsearch-server"
  ]
}