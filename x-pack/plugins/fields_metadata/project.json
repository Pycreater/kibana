{
  "$schema": "https://raw.githubusercontent.com/nrwl/nx/refs/heads/master/packages/nx/schemas/project-schema.json",
  "name": "@kbn/fields-metadata-plugin",
  "namedInputs": {
    "src": [
      "{workspaceRoot}/typings/**/*",
      "{projectRoot}/common/**/*",
      "{projectRoot}/public/**/*",
      "{projectRoot}/server/**/*",
      "{projectRoot}/types/**/*",
      "!{projectRoot}/target/**/*"
    ]
  },
  "targets": {
    "typecheck": {
      "inputs": [
        "src",
        "^src",
        "global:tsConfigs",
        "^global:tsConfigs"
      ],
      "outputs": [
        "{projectRoot}/target"
      ]
    }
  },
  "projectType": "application",
  "sourceRoot": "x-pack/plugins/fields_metadata",
  "implicitDependencies": [
    "@kbn/core",
    "@kbn/io-ts-utils",
    "@kbn/logging",
    "@kbn/core-http-request-handler-context-server",
    "@kbn/core-http-server",
    "@kbn/logging-mocks"
  ]
}