{
  "$schema": "https://raw.githubusercontent.com/nrwl/nx/refs/heads/master/packages/nx/schemas/project-schema.json",
  "name": "@kbn/banners-plugin",
  "namedInputs": {
    "src": [
      "{projectRoot}/public/**/*",
      "{projectRoot}/server/**/*",
      "{projectRoot}/common/**/*",
      "{workspaceRoot}/typings/**/*",
      "!{projectRoot}/target/**/*"
    ]
  },
  "targets": {
    "typecheck": {
      "inputs": [
        "src",
        "^src",
        "global:tsConfigs",
        "^global:tsConfigs"
      ],
      "outputs": [
        "{projectRoot}/target"
      ]
    }
  },
  "projectType": "application",
  "sourceRoot": "x-pack/plugins/banners",
  "implicitDependencies": [
    "@kbn/core",
    "@kbn/screenshot-mode-plugin",
    "@kbn/licensing-plugin",
    "@kbn/config-schema",
    "@kbn/i18n",
    "@kbn/shared-ux-markdown",
    "@kbn/react-kibana-mount"
  ]
}