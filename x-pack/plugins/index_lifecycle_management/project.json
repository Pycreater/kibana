{
  "$schema": "https://raw.githubusercontent.com/nrwl/nx/refs/heads/master/packages/nx/schemas/project-schema.json",
  "name": "@kbn/index-lifecycle-management-plugin",
  "namedInputs": {
    "src": [
      "{projectRoot}/__jest__/**/*",
      "{projectRoot}/integration_tests/**/*",
      "{projectRoot}/common/**/*",
      "{projectRoot}/public/**/*",
      "{projectRoot}/server/**/*",
      "{workspaceRoot}/typings/**/*",
      "!{projectRoot}/target/**/*"
    ]
  },
  "targets": {
    "typecheck": {
      "inputs": [
        "src",
        "^src",
        "global:tsConfigs",
        "^global:tsConfigs"
      ],
      "outputs": [
        "{projectRoot}/target"
      ]
    }
  },
  "projectType": "application",
  "sourceRoot": "x-pack/plugins/index_lifecycle_management",
  "implicitDependencies": [
    "@kbn/core",
    "@kbn/licensing-plugin",
    "@kbn/management-plugin",
    "@kbn/features-plugin",
    "@kbn/share-plugin",
    "@kbn/cloud-plugin",
    "@kbn/usage-collection-plugin",
    "@kbn/index-management-plugin",
    "@kbn/home-plugin",
    "@kbn/kibana-react-plugin",
    "@kbn/test-jest-helpers",
    "@kbn/core-http-browser-mocks",
    "@kbn/i18n",
    "@kbn/utility-types",
    "@kbn/analytics",
    "@kbn/es-ui-shared-plugin",
    "@kbn/i18n-react",
    "@kbn/core-http-browser",
    "@kbn/config-schema",
    "@kbn/shared-ux-router",
    "@kbn/ui-theme",
    "@kbn/shared-ux-link-redirect-app",
    "@kbn/index-management-shared-types",
    "@kbn/react-kibana-context-render",
    "@kbn/unsaved-changes-prompt",
    "@kbn/shared-ux-table-persist",
    "@kbn/index-lifecycle-management-common-shared"
  ]
}