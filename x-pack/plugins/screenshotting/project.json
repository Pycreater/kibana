{
  "$schema": "https://raw.githubusercontent.com/nrwl/nx/refs/heads/master/packages/nx/schemas/project-schema.json",
  "name": "@kbn/screenshotting-plugin",
  "namedInputs": {
    "src": [
      "{projectRoot}/common/**/*",
      "{projectRoot}/public/**/*",
      "{projectRoot}/server/**/*",
      "{workspaceRoot}/typings/**/*",
      "!{projectRoot}/target/**/*"
    ]
  },
  "targets": {
    "typecheck": {
      "inputs": [
        "src",
        "^src",
        "global:tsConfigs",
        "^global:tsConfigs"
      ],
      "outputs": [
        "{projectRoot}/target"
      ]
    }
  },
  "projectType": "application",
  "sourceRoot": "x-pack/plugins/screenshotting",
  "implicitDependencies": [
    "@kbn/core",
    "@kbn/expressions-plugin",
    "@kbn/screenshot-mode-plugin",
    "@kbn/cloud-plugin",
    "@kbn/utility-types",
    "@kbn/logging",
    "@kbn/std",
    "@kbn/i18n",
    "@kbn/utils",
    "@kbn/core-logging-server-mocks",
    "@kbn/logging-mocks",
    "@kbn/core-http-server",
    "@kbn/core-plugins-server",
    "@kbn/task-manager-plugin",
    "@kbn/screenshotting-server"
  ]
}