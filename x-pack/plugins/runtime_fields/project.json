{
  "$schema": "https://raw.githubusercontent.com/nrwl/nx/refs/heads/master/packages/nx/schemas/project-schema.json",
  "name": "@kbn/runtime-fields-plugin",
  "namedInputs": {
    "src": [
      "{projectRoot}/public/**/*",
      "{workspaceRoot}/typings/**/*",
      "!{projectRoot}/target/**/*"
    ]
  },
  "targets": {
    "typecheck": {
      "inputs": [
        "src",
        "^src",
        "global:tsConfigs",
        "^global:tsConfigs"
      ],
      "outputs": [
        "{projectRoot}/target"
      ]
    }
  },
  "projectType": "application",
  "sourceRoot": "x-pack/plugins/runtime_fields",
  "implicitDependencies": [
    "@kbn/core",
    "@kbn/es-ui-shared-plugin",
    "@kbn/kibana-react-plugin",
    "@kbn/test-jest-helpers",
    "@kbn/i18n",
    "@kbn/i18n-react",
    "@kbn/monaco",
    "@kbn/code-editor",
    "@kbn/code-editor-mock",
    "@kbn/react-kibana-mount"
  ]
}