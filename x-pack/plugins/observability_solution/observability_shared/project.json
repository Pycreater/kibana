{
  "$schema": "https://raw.githubusercontent.com/nrwl/nx/refs/heads/master/packages/nx/schemas/project-schema.json",
  "name": "@kbn/observability-shared-plugin",
  "namedInputs": {
    "src": [
      "{projectRoot}/common/**/*",
      "{projectRoot}/public/**/*",
      "{projectRoot}/public/**/*.json",
      "{projectRoot}/server/**/*",
      "{workspaceRoot}/typings/**/*",
      "{workspaceRoot}/typings/**/*",
      "{projectRoot}/.storybook/**/*",
      "!{projectRoot}/target/**/*",
      "!{projectRoot}/.storybook/**/*.js"
    ]
  },
  "targets": {
    "typecheck": {
      "inputs": [
        "src",
        "^src",
        "global:tsConfigs",
        "^global:tsConfigs"
      ],
      "outputs": [
        "{projectRoot}/target"
      ]
    }
  },
  "projectType": "application",
  "sourceRoot": "x-pack/plugins/observability_solution/observability_shared",
