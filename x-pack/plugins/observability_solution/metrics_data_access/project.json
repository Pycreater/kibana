{
  "$schema": "https://raw.githubusercontent.com/nrwl/nx/refs/heads/master/packages/nx/schemas/project-schema.json",
  "name": "@kbn/metrics-data-access-plugin",
  "namedInputs": {
    "src": [
      "{workspaceRoot}/typings/**/*",
      "{projectRoot}/server/**/*",
      "{projectRoot}/common/**/*",
      "{projectRoot}/public/**/*",
      "!{projectRoot}/target/**/*"
    ]
  },
  "targets": {
    "typecheck": {
      "inputs": [
        "src",
        "^src",
        "global:tsConfigs",
        "^global:tsConfigs"
      ],
      "outputs": [
        "{projectRoot}/target"
      ]
    }
  },
  "projectType": "application",
  "sourceRoot": "x-pack/plugins/observability_solution/metrics_data_access",
  "implicitDependencies": [
    "@kbn/core",
    "@kbn/core-saved-objects-api-server",
    "@kbn/io-ts-utils",
