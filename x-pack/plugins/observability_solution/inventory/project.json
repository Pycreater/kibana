{
  "$schema": "https://raw.githubusercontent.com/nrwl/nx/refs/heads/master/packages/nx/schemas/project-schema.json",
  "name": "@kbn/inventory-plugin",
  "namedInputs": {
    "src": [
      "{workspaceRoot}/typings/**/*",
      "{projectRoot}/common/**/*",
      "{projectRoot}/public/**/*",
      "{workspaceRoot}/typings/**/*",
      "{projectRoot}/public/**/*.json",
      "{projectRoot}/server/**/*",
      "{projectRoot}/.storybook/**/*",
      "!{projectRoot}/target/**/*",
      "!{projectRoot}/.storybook/**/*.js"
    ]
  },
  "targets": {
    "typecheck": {
      "inputs": [
        "src",
        "^src",
        "global:tsConfigs",
        "^global:tsConfigs"
      ],
      "outputs": [
        "{projectRoot}/target"
      ]
    }
  },
  "projectType": "application",
  "sourceRoot": "x-pack/plugins/observability_solution/inventory",
