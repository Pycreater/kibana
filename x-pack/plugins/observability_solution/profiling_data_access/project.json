{
  "$schema": "https://raw.githubusercontent.com/nrwl/nx/refs/heads/master/packages/nx/schemas/project-schema.json",
  "name": "@kbn/profiling-data-access-plugin",
  "namedInputs": {
    "src": [
      "{projectRoot}/common/**/*",
      "{projectRoot}/server/**/*",
      "{projectRoot}/jest.config.js",
      "!{projectRoot}/target/**/*"
    ]
  },
  "targets": {
    "typecheck": {
      "inputs": [
        "src",
        "^src",
        "global:tsConfigs",
        "^global:tsConfigs"
      ],
      "outputs": [
        "{projectRoot}/target"
      ]
    }
  },
  "projectType": "application",
  "sourceRoot": "x-pack/plugins/observability_solution/profiling_data_access",
  "implicitDependencies": [
    "@kbn/config-schema",
    "@kbn/core",
    "@kbn/es-query",
    "@kbn/es-types",
    "@kbn/apm-utils",
    "@kbn/profiling-utils",
    "@kbn/fleet-plugin",
    "@kbn/cloud-plugin",
    "@kbn/spaces-plugin",
    "@kbn/observability-plugin"
  ]
}