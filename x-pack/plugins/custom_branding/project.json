{
  "$schema": "https://raw.githubusercontent.com/nrwl/nx/refs/heads/master/packages/nx/schemas/project-schema.json",
  "name": "@kbn/custom-branding-plugin",
  "namedInputs": {
    "src": [
      "{projectRoot}/public/**/*",
      "{projectRoot}/server/**/*",
      "{projectRoot}/common/**/*",
      "!{projectRoot}/target/**/*"
    ]
  },
  "targets": {
    "typecheck": {
      "inputs": [
        "src",
        "^src",
        "global:tsConfigs",
        "^global:tsConfigs"
      ],
      "outputs": [
        "{projectRoot}/target"
      ]
    }
  },
  "projectType": "application",
  "sourceRoot": "x-pack/plugins/custom_branding",
  "implicitDependencies": [
    "@kbn/core",
    "@kbn/licensing-plugin",
    "@kbn/license-api-guard-plugin",
    "@kbn/i18n",
    "@kbn/core-http-request-handler-context-server",
    "@kbn/core-custom-branding-common",
    "@kbn/core-lifecycle-server",
    "@kbn/core-ui-settings-common",
    "@kbn/config-schema"
  ]
}