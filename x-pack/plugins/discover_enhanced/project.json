{
  "$schema": "https://raw.githubusercontent.com/nrwl/nx/refs/heads/master/packages/nx/schemas/project-schema.json",
  "name": "@kbn/discover-enhanced-plugin",
  "namedInputs": {
    "src": [
      "{projectRoot}/*.ts",
      "{projectRoot}/common/**/*",
      "{projectRoot}/public/**/*",
      "{projectRoot}/server/**/*",
      "!{projectRoot}/target/**/*"
    ]
  },
  "targets": {
    "typecheck": {
      "inputs": [
        "src",
        "^src",
        "global:tsConfigs",
        "^global:tsConfigs"
      ],
      "outputs": [
        "{projectRoot}/target"
      ]
    }
  },
  "projectType": "application",
  "sourceRoot": "x-pack/plugins/discover_enhanced",
  "implicitDependencies": [
    "@kbn/core",
    "@kbn/data-plugin",
    "@kbn/discover-plugin",
    "@kbn/share-plugin",
    "@kbn/kibana-utils-plugin",
    "@kbn/lens-plugin",
    "@kbn/usage-collection-plugin",
    "@kbn/embeddable-plugin",
    "@kbn/visualizations-plugin",
    "@kbn/ui-actions-plugin",
    "@kbn/i18n",
    "@kbn/es-query",
    "@kbn/config-schema",
    "@kbn/presentation-publishing",
    "@kbn/data-views-plugin",
    "@kbn/unified-search-plugin"
  ]
}