{
  "$schema": "https://raw.githubusercontent.com/nrwl/nx/refs/heads/master/packages/nx/schemas/project-schema.json",
  "name": "@kbn/security-solution-ess",
  "namedInputs": {
    "src": [
      "{projectRoot}/index.ts",
      "{projectRoot}/common/**/*.ts",
      "{projectRoot}/public/**/*.ts",
      "{projectRoot}/public/**/*.tsx",
      "{projectRoot}/server/**/*.ts",
      "{workspaceRoot}/typings/**/*",
      "!{projectRoot}/target/**/*"
    ]
  },
  "targets": {
    "typecheck": {
      "inputs": [
        "src",
        "^src",
        "global:tsConfigs",
        "^global:tsConfigs"
      ],
      "outputs": [
        "{projectRoot}/target"
      ]
    }
  },
  "projectType": "application",
  "sourceRoot": "x-pack/plugins/security_solution_ess",
  "implicitDependencies": [
    "@kbn/core",
    "@kbn/config-schema",
    "@kbn/security-solution-plugin",
    "@kbn/kibana-react-plugin",
    "@kbn/security-solution-features",
    "@kbn/cases-plugin",
    "@kbn/security-solution-navigation",
    "@kbn/licensing-plugin",
    "@kbn/security-solution-upselling",
    "@kbn/i18n",
    "@kbn/navigation-plugin",
    "@kbn/management-plugin",
    "@kbn/core-chrome-browser"
  ]
}