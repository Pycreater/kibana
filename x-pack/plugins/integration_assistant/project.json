{
  "$schema": "https://raw.githubusercontent.com/nrwl/nx/refs/heads/master/packages/nx/schemas/project-schema.json",
  "name": "@kbn/integration-assistant-plugin",
  "namedInputs": {
    "src": [
      "{projectRoot}/index.ts",
      "{projectRoot}/public/**/*",
      "{projectRoot}/server/**/*.ts",
      "{projectRoot}/common/**/*.ts",
      "{projectRoot}/scripts/**/*.ts",
      "{projectRoot}/__jest__/**/*",
      "{workspaceRoot}/typings/**/*",
      "!{projectRoot}/target/**/*"
    ]
  },
  "targets": {
    "typecheck": {
      "inputs": [
        "src",
        "^src",
        "global:tsConfigs",
        "^global:tsConfigs"
      ],
      "outputs": [
        "{projectRoot}/target"
      ]
    },
    "jest": {
      "outputs": [
        "{projectRoot}/coverage"
      ],
      "options": {
        "jestConfig": "{projectRoot}/jest.config.js",
        "passWithNoTests": true
      }
    }
  },
  "projectType": "application",
  "sourceRoot": "x-pack/plugins/integration_assistant"
}