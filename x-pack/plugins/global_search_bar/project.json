{
  "$schema": "https://raw.githubusercontent.com/nrwl/nx/refs/heads/master/packages/nx/schemas/project-schema.json",
  "name": "@kbn/global-search-bar-plugin",
  "namedInputs": {
    "src": [
      "{projectRoot}/common/**/*",
      "{projectRoot}/public/**/*",
      "{projectRoot}/server/**/*",
      "!{projectRoot}/target/**/*"
    ]
  },
  "targets": {
    "typecheck": {
      "inputs": [
        "src",
        "^src",
        "global:tsConfigs",
        "^global:tsConfigs"
      ],
      "outputs": [
        "{projectRoot}/target"
      ]
    }
  },
  "projectType": "application",
  "sourceRoot": "x-pack/plugins/global_search_bar",
  "implicitDependencies": [
    "@kbn/core",
    "@kbn/usage-collection-plugin",
    "@kbn/global-search-plugin",
    "@kbn/saved-objects-tagging-plugin",
    "@kbn/analytics",
    "@kbn/i18n-react",
    "@kbn/i18n",
    "@kbn/saved-objects-tagging-oss-plugin",
    "@kbn/core-chrome-browser",
    "@kbn/react-kibana-context-render",
    "@kbn/config-schema"
  ]
}