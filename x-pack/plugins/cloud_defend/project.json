{
  "$schema": "https://raw.githubusercontent.com/nrwl/nx/refs/heads/master/packages/nx/schemas/project-schema.json",
  "name": "@kbn/cloud-defend-plugin",
  "namedInputs": {
    "src": [
      "{projectRoot}/common/**/*",
      "{projectRoot}/public/**/*",
      "{projectRoot}/server/**/*",
      "{workspaceRoot}/typings/**/*",
      "{projectRoot}/public/**/*.json",
      "{projectRoot}/server/**/*.json",
      "!{projectRoot}/target/**/*"
    ]
  },
  "targets": {
    "typecheck": {
      "inputs": [
        "src",
        "^src",
        "global:tsConfigs",
        "^global:tsConfigs"
      ],
      "outputs": [
        "{projectRoot}/target"
      ]
    }
  },
  "projectType": "application",
  "sourceRoot": "x-pack/plugins/cloud_defend",
  "implicitDependencies": [
    "@kbn/core",
    "@kbn/data-plugin",
    "@kbn/security-plugin",
    "@kbn/fleet-plugin",
    "@kbn/i18n-react",
    "@kbn/config-schema",
    "@kbn/licensing-plugin",
    "@kbn/data-plugin",
    "@kbn/kibana-react-plugin",
    "@kbn/monaco",
    "@kbn/i18n",
    "@kbn/usage-collection-plugin",
    "@kbn/cloud-plugin",
    "@kbn/shared-ux-router",
    "@kbn/shared-ux-link-redirect-app",
    "@kbn/core-logging-server-mocks",
    "@kbn/securitysolution-es-utils",
    "@kbn/es-types",
    "@kbn/data-views-plugin",
    "@kbn/utility-types",
    "@kbn/kubernetes-security-plugin",
    "@kbn/core-http-router-server-mocks",
    "@kbn/core-elasticsearch-server",
    "@kbn/code-editor",
    "@kbn/code-editor-mock",
    "@kbn/core-security-common"
  ]
}