{
  "$schema": "https://raw.githubusercontent.com/nrwl/nx/refs/heads/master/packages/nx/schemas/project-schema.json",
  "name": "@kbn/search-notebooks",
  "namedInputs": {
    "src": [
      "{projectRoot}/index.ts",
      "{projectRoot}/common/**/*.ts",
      "{projectRoot}/public/**/*.ts",
      "{projectRoot}/public/**/*.tsx",
      "{projectRoot}/server/**/*.ts",
      "{workspaceRoot}/typings/**/*",
      "!{projectRoot}/target/**/*"
    ]
  },
  "targets": {
    "typecheck": {
      "inputs": [
        "src",
        "^src",
        "global:tsConfigs",
        "^global:tsConfigs"
      ],
      "outputs": [
        "{projectRoot}/target"
      ]
    }
  },
  "projectType": "application",
  "sourceRoot": "x-pack/plugins/search_notebooks",
  "implicitDependencies": [
    "@kbn/analytics",
    "@kbn/config-schema",
    "@kbn/core",
    "@kbn/i18n",
    "@kbn/logging",
    "@kbn/console-plugin",
    "@kbn/i18n-react",
    "@kbn/kibana-react-plugin",
    "@kbn/react-kibana-context-theme",
    "@kbn/shared-ux-utility",
    "@kbn/kibana-utils-plugin",
    "@kbn/ipynb",
    "@kbn/usage-collection-plugin"
  ]
}