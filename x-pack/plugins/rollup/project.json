{
  "$schema": "https://raw.githubusercontent.com/nrwl/nx/refs/heads/master/packages/nx/schemas/project-schema.json",
  "name": "@kbn/rollup-plugin",
  "namedInputs": {
    "src": [
      "{projectRoot}/common/**/*",
      "{projectRoot}/fixtures/**/*",
      "{projectRoot}/public/**/*",
      "{projectRoot}/server/**/*",
      "!{projectRoot}/target/**/*"
    ]
  },
  "targets": {
    "typecheck": {
      "inputs": [
        "src",
        "^src",
        "global:tsConfigs",
        "^global:tsConfigs"
      ],
      "outputs": [
        "{projectRoot}/target"
      ]
    }
  },
  "projectType": "application",
  "sourceRoot": "x-pack/plugins/rollup",
  "implicitDependencies": [
    "@kbn/core",
    "@kbn/management-plugin",
    "@kbn/licensing-plugin",
    "@kbn/features-plugin",
    "@kbn/home-plugin",
    "@kbn/index-management-plugin",
    "@kbn/usage-collection-plugin",
    "@kbn/kibana-utils-plugin",
    "@kbn/kibana-react-plugin",
    "@kbn/es-ui-shared-plugin",
    "@kbn/data-plugin",
    "@kbn/test-jest-helpers",
    "@kbn/analytics",
    "@kbn/i18n",
    "@kbn/core-http-browser",
    "@kbn/i18n-react",
    "@kbn/config-schema",
    "@kbn/shared-ux-router",
    "@kbn/data-views-plugin",
    "@kbn/react-kibana-context-render",
    "@kbn/rollup"
  ]
}