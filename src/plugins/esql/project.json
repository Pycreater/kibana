{
  "$schema": "https://raw.githubusercontent.com/nrwl/nx/refs/heads/master/packages/nx/schemas/project-schema.json",
  "name": "@kbn/esql",
  "namedInputs": {
    "src": [
      "{workspaceRoot}/typings/**/*",
      "{projectRoot}/common/**/*",
      "{projectRoot}/public/**/*",
      "{projectRoot}/server/**/*",
      "!{projectRoot}/target/**/*"
    ]
  },
  "targets": {
    "typecheck": {
      "inputs": [
        "src",
        "^src",
        "global:tsConfigs",
        "^global:tsConfigs"
      ],
      "outputs": [
        "{projectRoot}/target"
      ]
    }
  },
  "projectType": "application",
  "sourceRoot": "src/plugins/esql",
  "implicitDependencies": [
    "@kbn/esql-editor",
    "@kbn/kibana-react-plugin",
    "@kbn/core",
    "@kbn/expressions-plugin",
    "@kbn/data-views-plugin",
    "@kbn/index-management-shared-types",
    "@kbn/i18n",
    "@kbn/config-schema",
    "@kbn/esql-utils",
    "@kbn/ui-actions-plugin",
    "@kbn/data-plugin",
    "@kbn/es-query",
    "@kbn/fields-metadata-plugin",
    "@kbn/usage-collection-plugin",
    "@kbn/content-management-plugin",
    "@kbn/kibana-utils-plugin"
  ]
}