{
  "$schema": "https://raw.githubusercontent.com/nrwl/nx/refs/heads/master/packages/nx/schemas/project-schema.json",
  "name": "@kbn/event-annotation-plugin",
  "namedInputs": {
    "src": [
      "{projectRoot}/common/**/*",
      "{projectRoot}/public/**/*",
      "{projectRoot}/server/**/*",
      "!{projectRoot}/target/**/*"
    ]
  },
  "targets": {
    "typecheck": {
      "inputs": [
        "src",
        "^src",
        "global:tsConfigs",
        "^global:tsConfigs"
      ],
      "outputs": [
        "{projectRoot}/target"
      ]
    }
  },
  "projectType": "application",
  "sourceRoot": "src/plugins/event_annotation",
  "implicitDependencies": [
    "@kbn/core",
    "@kbn/expressions-plugin",
    "@kbn/data-plugin",
    "@kbn/i18n",
    "@kbn/data-views-plugin",
    "@kbn/inspector-plugin",
    "@kbn/core-ui-settings-browser",
    "@kbn/datemath",
    "@kbn/saved-objects-finder-plugin",
    "@kbn/saved-objects-tagging-plugin",
    "@kbn/presentation-util-plugin",
    "@kbn/visualizations-plugin",
    "@kbn/data-views-plugin",
    "@kbn/visualization-ui-components",
    "@kbn/i18n-react",
    "@kbn/core-saved-objects-server",
    "@kbn/unified-search-plugin",
    "@kbn/core-saved-objects-server",
    "@kbn/object-versioning",
    "@kbn/config-schema",
    "@kbn/content-management-plugin",
    "@kbn/event-annotation-components",
    "@kbn/event-annotation-common",
    "@kbn/content-management-utils",
    "@kbn/logging",
    "@kbn/core-plugins-server"
  ]
}