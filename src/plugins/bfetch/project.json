{
  "$schema": "https://raw.githubusercontent.com/nrwl/nx/refs/heads/master/packages/nx/schemas/project-schema.json",
  "name": "@kbn/bfetch-plugin",
  "namedInputs": {
    "src": [
      "{projectRoot}/common/**/*",
      "{projectRoot}/public/**/*",
      "{projectRoot}/server/**/*",
      "{projectRoot}/index.ts",
      "!{projectRoot}/target/**/*"
    ]
  },
  "targets": {
    "typecheck": {
      "inputs": [
        "src",
        "^src",
        "global:tsConfigs",
        "^global:tsConfigs"
      ],
      "outputs": [
        "{projectRoot}/target"
      ]
    }
  },
  "projectType": "application",
  "sourceRoot": "src/plugins/bfetch",
  "implicitDependencies": [
    "@kbn/core",
    "@kbn/kibana-utils-plugin",
    "@kbn/i18n",
    "@kbn/config-schema",
    "@kbn/std",
    "@kbn/core-http-common",
    "@kbn/bfetch-error",
    "@kbn/ebt-tools",
    "@kbn/item-buffer"
  ]
}