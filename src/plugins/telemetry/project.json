{
  "$schema": "https://raw.githubusercontent.com/nrwl/nx/refs/heads/master/packages/nx/schemas/project-schema.json",
  "name": "@kbn/telemetry-plugin",
  "namedInputs": {
    "src": [
      "{projectRoot}/public/**/**/*",
      "{projectRoot}/server/**/**/*",
      "{projectRoot}/common/**/*",
      "{workspaceRoot}/typings/**/*",
      "{projectRoot}/schema/oss_plugins.json",
      "{projectRoot}/schema/oss_root.json",
      "{projectRoot}/schema/kbn_packages.json",
      "!{projectRoot}/target/**/*"
    ]
  },
  "targets": {
    "typecheck": {
      "inputs": [
        "src",
        "^src",
        "global:tsConfigs",
        "^global:tsConfigs"
      ],
      "outputs": [
        "{projectRoot}/target"
      ]
    }
  },
  "projectType": "application",
  "sourceRoot": "src/plugins/telemetry",
  "implicitDependencies": [
    "@kbn/core",
    "@kbn/home-plugin",
    "@kbn/kibana-utils-plugin",
    "@kbn/screenshot-mode-plugin",
    "@kbn/telemetry-collection-manager-plugin",
    "@kbn/usage-collection-plugin",
    "@kbn/security-plugin",
    "@kbn/test-jest-helpers",
    "@kbn/shared-ux-utility",
    "@kbn/i18n",
    "@kbn/i18n-react",
    "@kbn/config-schema",
    "@kbn/utils",
    "@kbn/core-saved-objects-server",
    "@kbn/core-saved-objects-api-server",
    "@kbn/std",
    "@kbn/core-http-browser-mocks",
    "@kbn/core-http-browser",
    "@kbn/core-http-server",
    "@kbn/analytics-collection-utils",
    "@kbn/react-kibana-mount",
    "@kbn/core-node-server",
    "@kbn/security-plugin-types-server"
  ]
}