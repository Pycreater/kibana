{
  "$schema": "https://raw.githubusercontent.com/nrwl/nx/refs/heads/master/packages/nx/schemas/project-schema.json",
  "name": "@kbn/ui-actions-enhanced-plugin",
  "namedInputs": {
    "src": [
      "{projectRoot}/public/**/*",
      "{projectRoot}/server/**/*",
      "{projectRoot}/common/**/*",
      "{workspaceRoot}/typings/**/*",
      "!{projectRoot}/target/**/*"
    ]
  },
  "targets": {
    "typecheck": {
      "inputs": [
        "src",
        "^src",
        "global:tsConfigs",
        "^global:tsConfigs"
      ],
      "outputs": [
        "{projectRoot}/target"
      ]
    }
  },
  "projectType": "application",
  "sourceRoot": "src/plugins/ui_actions_enhanced",
  "implicitDependencies": [
    "@kbn/core",
    "@kbn/data-plugin",
    "@kbn/embeddable-plugin",
    "@kbn/kibana-react-plugin",
    "@kbn/kibana-utils-plugin",
    "@kbn/ui-actions-plugin",
    "@kbn/licensing-plugin",
    "@kbn/i18n",
    "@kbn/utility-types",
    "@kbn/i18n-react",
    "@kbn/handlebars",
    "@kbn/rison",
    "@kbn/datemath",
    "@kbn/monaco",
    "@kbn/presentation-publishing"
  ]
}