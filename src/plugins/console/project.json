{
  "$schema": "https://raw.githubusercontent.com/nrwl/nx/refs/heads/master/packages/nx/schemas/project-schema.json",
  "name": "@kbn/console-plugin",
  "namedInputs": {
    "src": [
      "{projectRoot}/common/**/*",
      "{projectRoot}/public/**/*",
      "{projectRoot}/server/**/*",
      "!{projectRoot}/target/**/*"
    ]
  },
  "targets": {
    "typecheck": {
      "inputs": [
        "src",
        "^src",
        "global:tsConfigs",
        "^global:tsConfigs"
      ],
      "outputs": [
        "{projectRoot}/target"
      ]
    }
  },
  "projectType": "application",
  "sourceRoot": "src/plugins/console",
  "implicitDependencies": [
    "@kbn/core",
    "@kbn/dev-tools-plugin",
    "@kbn/es-ui-shared-plugin",
    "@kbn/home-plugin",
    "@kbn/kibana-utils-plugin",
    "@kbn/share-plugin",
    "@kbn/usage-collection-plugin",
    "@kbn/i18n",
    "@kbn/analytics",
    "@kbn/utility-types",
    "@kbn/i18n-react",
    "@kbn/shared-ux-utility",
    "@kbn/core-http-browser",
    "@kbn/config-schema",
    "@kbn/core-http-router-server-internal",
    "@kbn/core-elasticsearch-server",
    "@kbn/core-http-browser-mocks",
    "@kbn/react-kibana-context-theme",
    "@kbn/code-editor",
    "@kbn/monaco",
    "@kbn/cloud-plugin",
    "@kbn/core-notifications-browser",
    "@kbn/react-kibana-context-render",
    "@kbn/react-kibana-mount",
    "@kbn/ui-theme",
    "@kbn/core-doc-links-browser",
    "@kbn/shared-ux-router"
  ]
}