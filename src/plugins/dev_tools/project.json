{
  "$schema": "https://raw.githubusercontent.com/nrwl/nx/refs/heads/master/packages/nx/schemas/project-schema.json",
  "name": "@kbn/dev-tools-plugin",
  "namedInputs": {
    "src": [
      "{projectRoot}/common/*",
      "{projectRoot}/public/**/*",
      "{projectRoot}/server/**/*",
      "!{projectRoot}/target/**/*"
    ]
  },
  "targets": {
    "typecheck": {
      "inputs": [
        "src",
        "^src",
        "global:tsConfigs",
        "^global:tsConfigs"
      ],
      "outputs": [
        "{projectRoot}/target"
      ]
    }
  },
  "projectType": "application",
  "sourceRoot": "src/plugins/dev_tools",
  "implicitDependencies": [
    "@kbn/core",
    "@kbn/url-forwarding-plugin",
    "@kbn/management-plugin",
    "@kbn/i18n",
    "@kbn/ui-theme",
    "@kbn/shared-ux-router",
    "@kbn/deeplinks-devtools",
    "@kbn/config-schema",
    "@kbn/core-plugins-server",
    "@kbn/react-kibana-context-render"
  ]
}