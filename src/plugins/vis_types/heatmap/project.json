{
  "$schema": "https://raw.githubusercontent.com/nrwl/nx/refs/heads/master/packages/nx/schemas/project-schema.json",
  "name": "@kbn/vis-type-heatmap-plugin",
  "namedInputs": {
    "src": [
      "{projectRoot}/common/**/*",
      "{projectRoot}/public/**/*",
      "{projectRoot}/server/**/*",
      "{projectRoot}/*.ts",
      "!{projectRoot}/target/**/*"
    ]
  },
  "targets": {
    "typecheck": {
      "inputs": [
        "src",
        "^src",
        "global:tsConfigs",
        "^global:tsConfigs"
      ],
      "outputs": [
        "{projectRoot}/target"
      ]
    }
  },
  "projectType": "application",
  "sourceRoot": "src/plugins/vis_types/heatmap",
  "implicitDependencies": [
    "@kbn/core",
    "@kbn/charts-plugin",
    "@kbn/data-plugin",
    "@kbn/expressions-plugin",
    "@kbn/visualizations-plugin",
    "@kbn/usage-collection-plugin",
    "@kbn/vis-default-editor-plugin",
    "@kbn/field-formats-plugin",
    "@kbn/data-views-plugin",
    "@kbn/kibana-utils-plugin",
    "@kbn/i18n",
    "@kbn/i18n-react",
    "@kbn/test-jest-helpers",
    "@kbn/config-schema"
  ]
}