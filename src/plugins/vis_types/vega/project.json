{
  "$schema": "https://raw.githubusercontent.com/nrwl/nx/refs/heads/master/packages/nx/schemas/project-schema.json",
  "name": "@kbn/vis-type-vega-plugin",
  "namedInputs": {
    "src": [
      "{projectRoot}/server/**/*",
      "{projectRoot}/public/**/*",
      "{projectRoot}/*.ts",
      "{projectRoot}/public/test_utils/vega_map_test.json",
      "{projectRoot}/public/test_utils/vegalite_graph.json",
      "{projectRoot}/public/test_utils/vega_graph.json",
      "!{projectRoot}/target/**/*"
    ]
  },
  "targets": {
    "typecheck": {
      "inputs": [
        "src",
        "^src",
        "global:tsConfigs",
        "^global:tsConfigs"
      ],
      "outputs": [
        "{projectRoot}/target"
      ]
    }
  },
  "projectType": "application",
  "sourceRoot": "src/plugins/vis_types/vega"
}