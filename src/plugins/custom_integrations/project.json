{
  "$schema": "https://raw.githubusercontent.com/nrwl/nx/refs/heads/master/packages/nx/schemas/project-schema.json",
  "name": "@kbn/custom-integrations-plugin",
  "namedInputs": {
    "src": [
      "{workspaceRoot}/typings/**/*",
      "{projectRoot}/common/**/*",
      "{projectRoot}/public/**/*",
      "{projectRoot}/server/**/*",
      "{projectRoot}/storybook/**/*",
      "!{projectRoot}/target/**/*"
    ]
  },
  "targets": {
    "typecheck": {
      "inputs": [
        "src",
        "^src",
        "global:tsConfigs",
        "^global:tsConfigs"
      ],
      "outputs": [
        "{projectRoot}/target"
      ]
    }
  },
  "projectType": "application",
  "sourceRoot": "src/plugins/custom_integrations",
  "implicitDependencies": [
    "@kbn/core",
    "@kbn/kibana-react-plugin",
    "@kbn/i18n",
    "@kbn/i18n-react",
    "@kbn/ui-theme",
    "@kbn/logging-mocks",
    "@kbn/config-schema",
    "@kbn/storybook"
  ]
}