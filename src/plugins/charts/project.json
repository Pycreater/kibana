{
  "$schema": "https://raw.githubusercontent.com/nrwl/nx/refs/heads/master/packages/nx/schemas/project-schema.json",
  "name": "@kbn/charts-plugin",
  "namedInputs": {
    "src": [
      "{projectRoot}/common/**/*",
      "{projectRoot}/public/**/*",
      "{projectRoot}/server/**/*",
      "!{projectRoot}/target/**/*"
    ]
  },
  "targets": {
    "typecheck": {
      "inputs": [
        "src",
        "^src",
        "global:tsConfigs",
        "^global:tsConfigs"
      ],
      "outputs": [
        "{projectRoot}/target"
      ]
    }
  },
  "projectType": "application",
  "sourceRoot": "src/plugins/charts",
  "implicitDependencies": [
    "@kbn/core",
    "@kbn/expressions-plugin",
    "@kbn/embeddable-plugin",
    "@kbn/coloring",
    "@kbn/i18n",
    "@kbn/utility-types",
    "@kbn/i18n-react",
    "@kbn/ui-theme",
    "@kbn/shared-ux-utility",
    "@kbn/config-schema",
    "@kbn/data-plugin"
  ]
}