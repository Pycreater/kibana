{
  "$schema": "https://raw.githubusercontent.com/nrwl/nx/refs/heads/master/packages/nx/schemas/project-schema.json",
  "name": "@kbn/content-management-plugin",
  "namedInputs": {
    "src": [
      "{projectRoot}/common/**/*",
      "{projectRoot}/public/**/*",
      "{projectRoot}/server/**/*",
      "!{projectRoot}/target/**/*"
    ]
  },
  "targets": {
    "typecheck": {
      "inputs": [
        "src",
        "^src",
        "global:tsConfigs",
        "^global:tsConfigs"
      ],
      "outputs": [
        "{projectRoot}/target"
      ]
    }
  },
  "projectType": "application",
  "sourceRoot": "src/plugins/content_management",
  "implicitDependencies": [
    "@kbn/core",
    "@kbn/config-schema",
    "@kbn/core-http-request-handler-context-server",
    "@kbn/es-query",
    "@kbn/core-test-helpers-kbn-server",
    "@kbn/item-buffer",
    "@kbn/object-versioning",
    "@kbn/core-saved-objects-api-server-mocks",
    "@kbn/core-saved-objects-api-server",
    "@kbn/saved-objects-settings",
    "@kbn/core-http-server",
    "@kbn/content-management-favorites-server",
    "@kbn/usage-collection-plugin",
    "@kbn/object-versioning-utils"
  ]
}