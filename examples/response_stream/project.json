{
  "$schema": "https://raw.githubusercontent.com/nrwl/nx/refs/heads/master/packages/nx/schemas/project-schema.json",
  "name": "@kbn/response-stream-plugin",
  "namedInputs": {
    "src": [
      "{projectRoot}/index.ts",
      "{projectRoot}/common/**/*.ts",
      "{projectRoot}/public/**/*.ts",
      "{projectRoot}/public/**/*.tsx",
      "{projectRoot}/server/**/*.ts",
      "{workspaceRoot}/typings/**/*",
      "!{projectRoot}/target/**/*"
    ]
  },
  "targets": {
    "typecheck": {
      "inputs": [
        "src",
        "^src",
        "global:tsConfigs",
        "^global:tsConfigs"
      ],
      "outputs": [
        "{projectRoot}/target"
      ]
    }
  },
  "projectType": "application",
  "sourceRoot": "examples/response_stream",
  "implicitDependencies": [
    "@kbn/core",
    "@kbn/developer-examples-plugin",
    "@kbn/data-plugin",
    "@kbn/kibana-react-plugin",
    "@kbn/config-schema",
    "@kbn/shared-ux-router",
    "@kbn/ml-response-stream",
    "@kbn/react-kibana-context-render"
  ]
}