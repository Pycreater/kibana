{
  "$schema": "https://raw.githubusercontent.com/nrwl/nx/refs/heads/master/packages/nx/schemas/project-schema.json",
  "name": "@kbn/discover-customization-examples-plugin",
  "namedInputs": {
    "src": [
      "{projectRoot}/common/**/*",
      "{projectRoot}/public/**/*",
      "{projectRoot}/server/**/*",
      "{workspaceRoot}/typings/**/*",
      "!{projectRoot}/target/**/*"
    ]
  },
  "targets": {
    "typecheck": {
      "inputs": [
        "src",
        "^src",
        "global:tsConfigs",
        "^global:tsConfigs"
      ],
      "outputs": [
        "{projectRoot}/target"
      ]
    }
  },
  "projectType": "application",
  "sourceRoot": "examples/discover_customization_examples",
  "implicitDependencies": [
    "@kbn/core",
    "@kbn/discover-plugin",
    "@kbn/developer-examples-plugin",
    "@kbn/controls-plugin",
    "@kbn/shared-ux-router",
    "@kbn/i18n-react",
    "@kbn/react-kibana-context-theme",
    "@kbn/data-plugin"
  ]
}