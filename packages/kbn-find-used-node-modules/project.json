{
  "$schema": "https://raw.githubusercontent.com/nrwl/nx/refs/heads/master/packages/nx/schemas/project-schema.json",
  "name": "@kbn/find-used-node-modules",
  "namedInputs": {
    "src": [
      "{projectRoot}/**/*.ts",
      "!{projectRoot}/target/**/*"
    ]
  },
  "targets": {
    "typecheck": {
      "inputs": [
        "src",
        "^src",
        "global:tsConfigs",
        "^global:tsConfigs"
      ],
      "outputs": [
        "{projectRoot}/target"
      ]
    }
  },
  "projectType": "library",
  "sourceRoot": "packages/kbn-find-used-node-modules",
  "implicitDependencies": [
    "@kbn/import-resolver",
    "@kbn/babel-preset",
    "@kbn/import-locator",
    "@kbn/repo-info"
  ]
}