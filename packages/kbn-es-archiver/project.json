{
  "$schema": "https://raw.githubusercontent.com/nrwl/nx/refs/heads/master/packages/nx/schemas/project-schema.json",
  "name": "@kbn/es-archiver",
  "namedInputs": {
    "src": [
      "{projectRoot}/**/*.ts",
      "{projectRoot}/src/**/*.json",
      "!{projectRoot}/target/**/*"
    ]
  },
  "targets": {
    "typecheck": {
      "inputs": [
        "src",
        "^src",
        "global:tsConfigs",
        "^global:tsConfigs"
      ],
      "outputs": [
        "{projectRoot}/target"
      ]
    }
  },
  "projectType": "library",
  "sourceRoot": "packages/kbn-es-archiver",
  "implicitDependencies": [
    "@kbn/core-saved-objects-server",
    "@kbn/dev-utils",
    "@kbn/test",
    "@kbn/tooling-log",
    "@kbn/utils",
    "@kbn/dev-cli-runner",
    "@kbn/dev-cli-errors",
    "@kbn/repo-info",
    "@kbn/jest-serializers"
  ]
}