{
  "$schema": "https://raw.githubusercontent.com/nrwl/nx/refs/heads/master/packages/nx/schemas/project-schema.json",
  "name": "@kbn/storybook",
  "namedInputs": {
    "src": [
      "{projectRoot}/**/*.ts",
      "{projectRoot}/**/*.tsx",
      "!{projectRoot}/target/**/*"
    ]
  },
  "targets": {
    "typecheck": {
      "inputs": [
        "src",
        "^src",
        "global:tsConfigs",
        "^global:tsConfigs"
      ],
      "outputs": [
        "{projectRoot}/target"
      ]
    }
  },
  "projectType": "library",
  "sourceRoot": "packages/kbn-storybook",
  "implicitDependencies": [
    "@kbn/ui-shared-deps-npm",
    "@kbn/ui-shared-deps-src",
    "@kbn/repo-info",
    "@kbn/dev-cli-runner",
    "@kbn/core-theme-browser",
    "@kbn/i18n",
    "@kbn/i18n-react",
    "@kbn/core-i18n-browser",
    "@kbn/react-kibana-context-root",
    "@kbn/core-analytics-browser"
  ]
}