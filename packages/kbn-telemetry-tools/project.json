{
  "$schema": "https://raw.githubusercontent.com/nrwl/nx/refs/heads/master/packages/nx/schemas/project-schema.json",
  "name": "@kbn/telemetry-tools",
  "namedInputs": {
    "src": [
      "{projectRoot}/**/*.ts",
      "!{projectRoot}/target/**/*"
    ]
  },
  "targets": {
    "typecheck": {
      "inputs": [
        "src",
        "^src",
        "global:tsConfigs",
        "^global:tsConfigs"
      ],
      "outputs": [
        "{projectRoot}/target"
      ]
    }
  },
  "projectType": "library",
  "sourceRoot": "packages/kbn-telemetry-tools",
  "implicitDependencies": [
    "@kbn/utility-types",
    "@kbn/dev-cli-runner",
    "@kbn/dev-cli-errors",
    "@kbn/repo-info",
    "@kbn/import-resolver",
    "@kbn/usage-collection-plugin",
    "@kbn/config-schema",
    "@kbn/safer-lodash-set"
  ]
}