{
  "$schema": "https://raw.githubusercontent.com/nrwl/nx/refs/heads/master/packages/nx/schemas/project-schema.json",
  "name": "@kbn/field-utils",
  "namedInputs": {
    "src": [
      "{projectRoot}/*.ts",
      "{projectRoot}/src/**/*",
      "{projectRoot}/__mocks__/**/*.ts",
      "!{projectRoot}/target/**/*"
    ]
  },
  "targets": {
    "typecheck": {
      "inputs": [
        "src",
        "^src",
        "global:tsConfigs",
        "^global:tsConfigs"
      ],
      "outputs": [
        "{projectRoot}/target"
      ]
    }
  },
  "projectType": "library",
  "sourceRoot": "packages/kbn-field-utils",
  "implicitDependencies": [
    "@kbn/i18n",
    "@kbn/data-views-plugin",
    "@kbn/react-field",
    "@kbn/field-types",
    "@kbn/expressions-plugin",
    "@kbn/data-view-utils",
    "@kbn/fields-metadata-plugin",
    "@kbn/shared-ux-markdown"
  ]
}