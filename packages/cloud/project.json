{
  "$schema": "https://raw.githubusercontent.com/nrwl/nx/refs/heads/master/packages/nx/schemas/project-schema.json",
  "name": "@kbn/cloud",
  "namedInputs": {
    "src": [
      "{projectRoot}/**/*.ts",
      "{projectRoot}/**/*.tsx",
      "!{projectRoot}/target/**/*"
    ]
  },
  "targets": {
    "typecheck": {
      "inputs": [
        "src",
        "^src",
        "global:tsConfigs",
        "^global:tsConfigs"
      ],
      "outputs": [
        "{projectRoot}/target"
      ]
    }
  },
  "projectType": "library",
  "sourceRoot": "packages/cloud",
  "implicitDependencies": [
    "@kbn/i18n",
    "@kbn/i18n-react",
    "@kbn/core-lifecycle-browser",
    "@kbn/cloud-plugin",
    "@kbn/share-plugin",
    "@kbn/security-plugin-types-server",
    "@kbn/react-kibana-context-render"
  ]
}