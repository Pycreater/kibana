{
  "$schema": "https://raw.githubusercontent.com/nrwl/nx/refs/heads/master/packages/nx/schemas/project-schema.json",
  "name": "@kbn/search-api-panels",
  "namedInputs": {
    "src": [
      "{projectRoot}/**/*.ts",
      "{projectRoot}/**/*.tsx",
      "!{projectRoot}/target/**/*"
    ]
  },
  "targets": {
    "typecheck": {
      "inputs": [
        "src",
        "^src",
        "global:tsConfigs",
        "^global:tsConfigs"
      ],
      "outputs": [
        "{projectRoot}/target"
      ]
    }
  },
  "projectType": "library",
  "sourceRoot": "packages/kbn-search-api-panels",
  "implicitDependencies": [
    "@kbn/i18n",
    "@kbn/core-application-browser",
    "@kbn/share-plugin",
    "@kbn/i18n-react",
    "@kbn/security-plugin",
    "@kbn/console-plugin",
    "@kbn/ui-theme",
    "@kbn/try-in-console",
    "@kbn/test-jest-helpers"
  ]
}