{
  "$schema": "https://raw.githubusercontent.com/nrwl/nx/refs/heads/master/packages/nx/schemas/project-schema.json",
  "name": "@kbn/unified-doc-viewer",
  "namedInputs": {
    "src": [
      "{projectRoot}/**/*.ts",
      "{projectRoot}/**/*.tsx",
      "!{projectRoot}/target/**/*"
    ]
  },
  "targets": {
    "typecheck": {
      "inputs": [
        "src",
        "^src",
        "global:tsConfigs",
        "^global:tsConfigs"
      ],
      "outputs": [
        "{projectRoot}/target"
      ]
    }
  },
  "projectType": "library",
  "sourceRoot": "packages/kbn-unified-doc-viewer",
  "implicitDependencies": [
    "@kbn/discover-utils",
    "@kbn/data-views-plugin",
    "@kbn/es-query",
    "@kbn/data-plugin",
    "@kbn/i18n-react",
    "@kbn/i18n",
    "@kbn/react-field",
    "@kbn/field-utils"
  ]
}