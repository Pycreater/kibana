{
  "name": "@kbn/nx",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/kbn-nx/src",
  "projectType": "library",
  "targets": {
    "_build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "{projectRoot}/target/types",
        "main": "packages/kbn-nx/src/index.ts",
        "tsConfig": "packages/kbn-nx/tsconfig.json",
        "assets": [
          "packages/kbn-nx/*.md",
          {
            "input": "./packages/kbn-nx/src",
            "glob": "**/!(*.ts)",
            "output": "./src"
          },
          {
            "input": "./packages/kbn-nx/src",
            "glob": "**/*.d.ts",
            "output": "./src"
          },
          {
            "input": "./packages/kbn-nx",
            "glob": "generators.json",
            "output": "."
          },
          {
            "input": "./packages/kbn-nx",
            "glob": "executors.json",
            "output": "."
          }
        ]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "packages/kbn-nx/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
