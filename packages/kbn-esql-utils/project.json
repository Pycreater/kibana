{
  "$schema": "https://raw.githubusercontent.com/nrwl/nx/refs/heads/master/packages/nx/schemas/project-schema.json",
  "name": "@kbn/esql-utils",
  "namedInputs": {
    "src": [
      "{projectRoot}/**/*.ts",
      "{projectRoot}/**/*.tsx",
      "!{projectRoot}/target/**/*"
    ]
  },
  "targets": {
    "typecheck": {
      "inputs": [
        "src",
        "^src",
        "global:tsConfigs",
        "^global:tsConfigs"
      ],
      "outputs": [
        "{projectRoot}/target"
      ]
    }
  },
  "projectType": "library",
  "sourceRoot": "packages/kbn-esql-utils",
  "implicitDependencies": [
    "@kbn/data-views-plugin",
    "@kbn/crypto-browser",
    "@kbn/data-view-utils",
    "@kbn/esql-ast",
    "@kbn/search-types",
    "@kbn/expressions-plugin",
    "@kbn/field-types",
    "@kbn/es-types",
    "@kbn/i18n",
    "@kbn/datemath",
    "@kbn/es-query"
  ]
}