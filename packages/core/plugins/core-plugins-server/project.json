{
  "$schema": "https://raw.githubusercontent.com/nrwl/nx/refs/heads/master/packages/nx/schemas/project-schema.json",
  "name": "@kbn/core-plugins-server",
  "namedInputs": {
    "src": [
      "{projectRoot}/**/*.ts",
      "!{projectRoot}/target/**/*"
    ]
  },
  "targets": {
    "typecheck": {
      "inputs": [
        "src",
        "^src",
        "global:tsConfigs",
        "^global:tsConfigs"
      ],
      "outputs": [
        "{projectRoot}/target"
      ]
    }
  },
  "projectType": "library",
  "sourceRoot": "packages/core/plugins/core-plugins-server",
  "implicitDependencies": [
    "@kbn/config",
    "@kbn/config-schema",
    "@kbn/utility-types",
    "@kbn/utils",
    "@kbn/logging",
    "@kbn/core-base-common",
    "@kbn/core-node-server",
    "@kbn/core-elasticsearch-server-internal",
    "@kbn/core-saved-objects-base-server-internal",
    "@kbn/core-lifecycle-server"
  ]
}