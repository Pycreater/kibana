{
  "$schema": "https://raw.githubusercontent.com/nrwl/nx/refs/heads/master/packages/nx/schemas/project-schema.json",
  "name": "@kbn/coloring",
  "namedInputs": {
    "src": [
      "{projectRoot}/**/*.ts",
      "{projectRoot}/**/*.tsx",
      "!{projectRoot}/target/**/*"
    ]
  },
  "targets": {
    "typecheck": {
      "inputs": [
        "src",
        "^src",
        "global:tsConfigs",
        "^global:tsConfigs"
      ],
      "outputs": [
        "{projectRoot}/target"
      ]
    }
  },
  "projectType": "library",
  "sourceRoot": "packages/kbn-coloring",
  "implicitDependencies": [
    "@kbn/i18n",
    "@kbn/i18n-react",
    "@kbn/interpreter",
    "@kbn/utility-types",
    "@kbn/shared-ux-utility",
    "@kbn/test-jest-helpers",
    "@kbn/data-plugin",
    "@kbn/ui-theme",
    "@kbn/visualization-utils"
  ]
}