{
  "$schema": "https://raw.githubusercontent.com/nrwl/nx/refs/heads/master/packages/nx/schemas/project-schema.json",
  "name": "@kbn/es",
  "namedInputs": {
    "src": [
      "{projectRoot}/**/*.ts",
      "{projectRoot}/**/*.js",
      "{projectRoot}/**/*.json",
      "!{projectRoot}/target/**/*"
    ]
  },
  "targets": {
    "typecheck": {
      "inputs": [
        "src",
        "^src",
        "global:tsConfigs",
        "^global:tsConfigs"
      ],
      "outputs": [
        "{projectRoot}/target"
      ]
    }
  },
  "projectType": "library",
  "sourceRoot": "packages/kbn-es",
  "implicitDependencies": [
    "@kbn/tooling-log",
    "@kbn/dev-utils",
    "@kbn/dev-proc-runner",
    "@kbn/ci-stats-reporter",
    "@kbn/mock-idp-utils",
    "@kbn/jest-serializers",
    "@kbn/repo-info"
  ]
}