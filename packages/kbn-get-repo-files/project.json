{
  "$schema": "https://raw.githubusercontent.com/nrwl/nx/refs/heads/master/packages/nx/schemas/project-schema.json",
  "name": "@kbn/get-repo-files",
  "namedInputs": {
    "src": [
      "{projectRoot}/**/*.js",
      "{projectRoot}/**/*.ts",
      "!{projectRoot}/target/**/*"
    ]
  },
  "targets": {
    "typecheck": {
      "inputs": [
        "src",
        "^src",
        "global:tsConfigs",
        "^global:tsConfigs"
      ],
      "outputs": [
        "{projectRoot}/target"
      ]
    }
  },
  "projectType": "library",
  "sourceRoot": "packages/kbn-get-repo-files",
  "implicitDependencies": [
    "@kbn/repo-info",
    "@kbn/repo-path",
    "@kbn/repo-packages"
  ]
}