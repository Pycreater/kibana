{
  "$schema": "https://raw.githubusercontent.com/nrwl/nx/refs/heads/master/packages/nx/schemas/project-schema.json",
  "name": "@kbn/test",
  "namedInputs": {
    "src": [
      "{projectRoot}/**/*.ts",
      "{projectRoot}/**/*.js",
      "{projectRoot}/src/jest/jest_flags.json",
      "!{projectRoot}/types/**/*",
      "!{projectRoot}/**/__fixtures__/**/*",
      "!{projectRoot}/target/**/*"
    ]
  },
  "targets": {
    "typecheck": {
      "inputs": [
        "src",
        "^src",
        "global:tsConfigs",
        "^global:tsConfigs"
      ],
      "outputs": [
        "{projectRoot}/target"
      ]
    }
  },
  "projectType": "library",
  "sourceRoot": "packages/kbn-test",
  "implicitDependencies": [
    "@kbn/dev-utils",
    "@kbn/std",
    "@kbn/tooling-log",
    "@kbn/get-repo-files",
    "@kbn/peggy",
    "@kbn/dev-cli-runner",
    "@kbn/dev-cli-errors",
    "@kbn/ci-stats-reporter",
    "@kbn/repo-info",
    "@kbn/es",
    "@kbn/dev-proc-runner",
    "@kbn/jest-serializers",
    "@kbn/stdio-dev-helpers",
    "@kbn/babel-register",
    "@kbn/repo-packages",
    "@kbn/core-saved-objects-api-server",
    "@kbn/mock-idp-utils",
    "@kbn/code-owners"
  ]
}